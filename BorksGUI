-- Scripts:

local function GUMFR_fake_script() -- UIButton.LocalScript 
	local script = Instance.new('LocalScript', UIButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
	local currentHealth = humanoid.Health
	
	script.Parent.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				game.ReplicatedStorage.Unblock.OnClientEvent:Connect(function()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, -10)
				end)
	
				game.ReplicatedStorage.Block.OnClientEvent:Connect(function()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, 10)
				end)
	
				while true do
					local Event = game:GetService("ReplicatedStorage").Block
					Event:FireServer()
					wait(0.0000001)							
				end		
			end				
		end	
	end)
end
coroutine.wrap(GUMFR_fake_script)()
local function NIDOH_fake_script() -- FlingButton.LocalScript 
	local script = Instance.new('LocalScript', FlingButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				game.Workspace.CurrentCamera.CameraSubject = v.Humanoid
				if v.Humanoid.Died then
					game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.HumanoidRootPart
				end
				
				script.Parent.Parent.Parent.Parent.Notification.Visible = true
				script.Parent.Parent.Parent.Parent.Notification.NotifDes.Text = "Reset to Stop Flinging (Only Works Sometimes so its WIP)"
				wait(5)
				script.Parent.Parent.Parent.Parent.Notification.Visible = false			
				
				while true do
					wait(0.0000001)
					game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0, 2)
					wait(0.0000001)
					game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0, -10000000000)
				end	
			end
			
		end
	end)
end
coroutine.wrap(NIDOH_fake_script)()
local function FSELU_fake_script() -- TeleportButton.LocalScript 
	local script = Instance.new('LocalScript', TeleportButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
			end
		end
		--game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace:FindFirstChild(PlayerName).HumanoidRootPart.CFrame * CFrame.new(0,0,0)
	end)
end
coroutine.wrap(FSELU_fake_script)()
local function OLCXEUW_fake_script() -- HealButton.LocalScript 
	local script = Instance.new('LocalScript', HealButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	
	local Players = game:FindService("Players")
	
	for i,v in pairs(Players:GetChildren()) do
		if v.Name == PlayerName then
			print("Player Found!")
		end
	end
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				local args = {
					[1] = "Light",
					[2] = -100,
					[3] = v.Humanoid,
					[4] = "TheyWouldntNeverSeeThis"
				}
				game:GetService("ReplicatedStorage").Damage:FireServer(unpack(args))	
			end
		end
	end)
end
coroutine.wrap(OLCXEUW_fake_script)()
local function DDBI_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	script.Parent.FocusLost:connect(function()
		for i,v in pairs(game.Players:GetChildren()) do
			if (string.sub(string.lower(v.Name),1,string.len(script.Parent.Text))) == string.lower(script.Parent.Text) then
				script.Parent.Text = v.Name
			end
		end
	end)
end
coroutine.wrap(DDBI_fake_script)()
