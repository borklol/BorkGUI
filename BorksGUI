local function CWUA_fake_script() -- DestroyGUI.LocalScript 
	local script = Instance.new('LocalScript', DestroyGUI)

	script.Parent.MouseButton1Click:Connect(function()
	script.Parent.Parent:Destroy()
	end)
end
