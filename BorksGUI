local function VHHHY_fake_script() -- TimeSkipFrame.TimeSkip 
	local script = Instance.new('LocalScript', TimeSkipFrame)

	local UIS = game:GetService("UserInputService")
	local debounce = false
	
	UIS.InputBegan:connect(function(Input,isTyping)
		local MouseInput = Input.UserInputType 
		local KeyCode = Input.KeyCode
	
		if KeyCode == Enum.KeyCode.V then
			if isTyping then
				return
			else
				if not debounce then
					
					debounce = true
					
					local s = Instance.new("Sound")
	
					s.Name = "Sound"
					s.SoundId = "rbxassetid://3381429040" -- Type your music ID here
					s.Volume = 1
					s.Pitch = 1
					s.Looped = false
					s.archivable = false
	
					s.Parent = game.Workspace
	
					wait(0.1)
	
					s:play()
					script.Parent.TimeErase1.Visible = true
					wait(.01)
					script.Parent.TimeErase2.Visible = true
					script.Parent.TimeErase1.Visible = false
					wait(.01)
					script.Parent.TimeErase3.Visible = true
					script.Parent.TimeErase2.Visible = false
					wait(.01)
					script.Parent.TimeErase4.Visible = true
					script.Parent.TimeErase3.Visible = false
					wait(.01)
					script.Parent.TimeErase5.Visible = true
					script.Parent.TimeErase4.Visible = false
					wait(.01)
					script.Parent.TimeErase6.Visible = true
					script.Parent.TimeErase5.Visible = false
					wait(.01)
					script.Parent.TimeErase7.Visible = true
					script.Parent.TimeErase6.Visible = false
					wait(.01)
					script.Parent.TimeErase8.Visible = true
					script.Parent.TimeErase7.Visible = false
					wait(.01)
					script.Parent.TimeErase9.Visible = true
					script.Parent.TimeErase8.Visible = false
					wait(.01)
					script.Parent.TimeErase10.Visible = true
					script.Parent.TimeErase9.Visible = false
					wait(.01)
					script.Parent.TimeErase11.Visible = true
					script.Parent.TimeErase10.Visible = false
					wait(.01)
					script.Parent.TimeErase12.Visible = true
					script.Parent.TimeErase11.Visible = false
					wait(.01)
					script.Parent.TimeErase13.Visible = true
					script.Parent.TimeErase12.Visible = false
					wait(.01)
					script.Parent.TimeErase14.Visible = true
					script.Parent.TimeErase13.Visible = false
					wait(.01)
					script.Parent.TimeErase15.Visible = true
					script.Parent.TimeErase14.Visible = false
					wait(.01)
