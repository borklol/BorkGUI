-- Scripts:

local function XRBO_fake_script() -- UIButton.LocalScript 
	local script = Instance.new('LocalScript', UIButton)

	script.Parent.Parent.UIPlayerBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.UIPlayerBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.UIPlayerBox.Text
	
	script.Parent.MouseButton1Click:Connect(function()
		game.ReplicatedStorage.Unblock.OnClientEvent:Connect(function()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.PlayerName.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, -10)
		end)
	
		game.ReplicatedStorage.Block.OnClientEvent:Connect(function()
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.PlayerName.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, 10)
		end)	
	end)
	
	a=game.Players:GetChildren()
	while true do
		local Event = game:GetService("ReplicatedStorage").Block
		Event:FireServer()
		wait(0.0000001)
	end
	
end
coroutine.wrap(XRBO_fake_script)()
local function VDGXJUA_fake_script() -- FlingButton.LocalScript 
	local script = Instance.new('LocalScript', FlingButton)

	script.Parent.Parent.FlingPlayerBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.FlingPlayerBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.FlingPlayerBox.Text
	
	script.Parent.MouseButton1Click:Connect(function()
		while true do
			wait(0.0000001)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, 2)
			wait(0.0000001)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, -10000000000)
		end
	end)
end
coroutine.wrap(VDGXJUA_fake_script)()
local function TXYNAIW_fake_script() -- RejoinGame.LocalScript 
	local script = Instance.new('LocalScript', RejoinGame)

	local ts = game:GetService("TeleportService")
	local p = game:GetService("Players").LocalPlayer
	
	script.Parent.MouseButton1Click:Connect(function()
		ts:Teleport(game.PlaceId, p)	
	end)
end
coroutine.wrap(TXYNAIW_fake_script)()
local function LIYW_fake_script() -- Frame.SmoothDrag 
	local script = Instance.new('LocalScript', Frame)

	local Drag = script.Parent.Parent.Frame
	gsCoreGui = game:GetService("CoreGui")
	gsTween = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
		local dragging
		local dragInput
		local dragStart
		local startPos
		local function update(input)
			local delta = input.Position - dragStart
			local dragTime = 0.04
			local SmoothDrag = {}
			SmoothDrag.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
			local dragSmoothFunction = gsTween:Create(Drag, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
			dragSmoothFunction:Play()
		end
		Drag.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = Drag.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)
		Drag.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging and Drag.Size then
				update(input)
			end
		end)
	
end
coroutine.wrap(LIYW_fake_script)()
local function XZWWAS_fake_script() -- OpenGUI.LocalScript 
	local script = Instance.new('LocalScript', OpenGUI)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Text == "Open GUI" then
			script.Parent.Parent.Frame.Visible = true
			script.Parent.Text = "Close GUI"
		else	
			script.Parent.Text = "Open GUI"
			script.Parent.Parent.Frame.Visible = false
		end
	end)
end
coroutine.wrap(XZWWAS_fake_script)()
