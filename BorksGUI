-- Scripts:

local function LESI_fake_script() -- OpenGUI.LocalScript 
	local script = Instance.new('LocalScript', OpenGUI)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Text == "Open GUI" then
			script.Parent.Parent.Pages.Visible = true
			script.Parent.Text = "Close GUI"
		else	
			script.Parent.Text = "Open GUI"
			script.Parent.Parent.Pages.Visible = false
		end
	end)
end
coroutine.wrap(LESI_fake_script)()
