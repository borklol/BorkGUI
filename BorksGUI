local function VIMHMVN_fake_script() -- UIButton.LocalScript 
	local script = Instance.new('LocalScript', UIButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox.Text)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
	local currentHealth = humanoid.Health
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Notification.Visible = true
		script.Parent.Parent.Parent.Parent.Notification.NotifDes.Text = "Kinda Buggy"
		wait(5)
		script.Parent.Parent.Parent.Parent.Notification.Visible = false	
	for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				game.ReplicatedStorage.Unblock.OnClientEvent:Connect(function()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, -10)
				end)
	
				game.ReplicatedStorage.Block.OnClientEvent:Connect(function()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.IusetobeMr_Goodiie.Character.HumanoidRootPart.CFrame * CFrame.new(0,0, 10)
				end)
	
				while true do
					local Event = game:GetService("ReplicatedStorage").Block
					Event:FireServer()
					wait(0.0000001)							
				end		
			end				
		end	
	end)
end
coroutine.wrap(VIMHMVN_fake_script)()
local function PFYS_fake_script() -- FlingButton.LocalScript 
	local script = Instance.new('LocalScript', FlingButton)

	script.Parent.Parent.TextBox.FocusLost:connect(
		function(enterPressed)
			if enterPressed then
				print(script.Parent.Parent.TextBox)
			end
		end
	)
	
	local PlayerName = script.Parent.Parent.TextBox
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetChildren()) do
			if v.Name == PlayerName.Text then
				script.Parent.Parent.Parent.Parent.Notification.Visible = true
				script.Parent.Parent.Parent.Parent.Notification.NotifDes.Text = "Kinda Buggy"
				wait(5)
				script.Parent.Parent.Parent.Parent.Notification.Visible = false			
				
				local args = {
					[1] = "Light",
					[2] = 0,
					[3] = v.Humanoid,
					[4] = "TheyWouldntNeverSeeThis"
				}
	
				game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = game.Workspace.Baseplate.CFrame * CFrame.new(0,10000000000, 10000000000)
				wait(.1)
				game:GetService("ReplicatedStorage").Damage:FireServer(unpack(args))
				wait(2.5)
				game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = game.Workspace.Baseplate.CFrame * CFrame.new(0,25,0)
				end	
			end
			
	end)
end
coroutine.wrap(PFYS_fake_script)()
